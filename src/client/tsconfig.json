{
  "extends": "../../tsconfig.json",
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "esnext",
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
